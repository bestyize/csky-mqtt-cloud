<?xml version="1.0" encoding="UTF-8"?>
<configWizard>
  <firstPage>pageNetCommCnpt</firstPage>
  <filePath>include/wiz_config.h</filePath>
  <!-- 网络组件选择 -->
  <page id="pageNetCommCnpt" summary="欢迎使用YoC sensor mqtt 工程创建向导" isLast="yes">
    <description>一款工作稳定广泛使用的有线以太网模块</description>
    <group id="groupid_nncc" text="网络组件">
      <widgets type="radiobutton" id="ccnpt_enc28j60" text="ENC28j60 SPI有线以太网模块" nextPage="pageend">
        <description>一款工作稳定广泛使用的有线以太网模块</description>
        <component id="yoc7.0#klwip_ch2201" lowVersion="V0.0.0" highVersion="V9.9.9"/>
        <component id="yoc7.0#yoc_kernel_ch2201" lowVersion="V0.0.0" highVersion="V9.9.9"/>
        <component id="yoc7.0#iot_alimqtt_tls_ch2201" lowVersion="V0.0.0" highVersion="V9.9.9"/>
        <fileConfig>CONFIG_TCPIP_ENC28J60</fileConfig>
      </widgets>
      <widgets type="radiobutton" id="ccnpt_esp8266" text="ESP8266 WiFi 模块" nextPage="pageend">
        <description>超低功耗的UART-WiFi 透传模块</description>
        <component id="yoc7.0#ksal_ch2201" lowVersion="V0.0.0" highVersion="V9.9.9"/>
        <component id="yoc7.0#yoc_kernel_ch2201" lowVersion="V0.0.0" highVersion="V9.9.9"/>
        <component id="yoc7.0#iot_alimqtt_tls_ch2201" lowVersion="V0.0.0" highVersion="V9.9.9"/>
        <fileConfig>CONFIG_SAL_ESP8266</fileConfig>
      </widgets>
    </group>
  </page>
  <initSelect>
    <widgets id="ccnpt_esp8266"/>
  </initSelect>
</configWizard>
